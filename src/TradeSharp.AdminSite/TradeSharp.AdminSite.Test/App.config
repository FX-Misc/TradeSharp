<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="log-file.txt" />
      <appendToFile value="true" />
      <maximumFileSize value="100KB" />
      <maxSizeRollBackups value="5" />
      <rollingStyle value="once" />
      <encoding value="windows-1251" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level: %date [%thread] {1} - %message%newline" />
      </layout>
    </appender>
    <appender name="Account" type="log4net.Appender.FileAppender">
      <file value="account-log.txt" />
      <appendToFile value="true" />
      <encoding value="windows-1251" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level: %date [%thread] {1} - %message%newline" />
      </layout>
    </appender>
    <logger name="RollingFile">
      <level value="ALL" />
      <appender-ref ref="RollingFile" />
    </logger>
    <logger name="Account">
      <level value="ALL" />
      <appender-ref ref="Account" />
    </logger>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="EntityFramework" publicKeyToken="b77a5c561934e089" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.0.0.0" newVersion="5.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <connectionStrings>
    <add name="TradeSharpConnection" connectionString="metadata=res://*/TradeSharp.csdl|res://*/TradeSharp.ssdl|res://*/TradeSharp.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=10.5.237.10;initial catalog=MTS_LIVE;user id=bse_user;password=Bse123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />

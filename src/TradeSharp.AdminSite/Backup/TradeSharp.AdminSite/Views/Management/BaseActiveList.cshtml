@using TradeSharp.SiteAdmin.App_GlobalResources
@model List<TradeSharp.Linq.COMMODITY>

<div id="content_table_inner">
    <table border="0" width="100%" cellpadding="0" cellspacing="0" id="product_table">
        <tr>
            <th class="table-header-repeat line-left"><span>@Resource.TitleName</span></th>
            <th class="table-header-repeat line-left"><span>@Resource.TitleComment</span></th>
            <th class="table-header-repeat line-left"><span>@Resource.TitleCode</span></th>
        </tr>
        @foreach (var active in Model)
        {
            <tr>
                <td>
                    <a href="javascript:void(0);" class="table-item" onclick="getUrlEncodedRowValues(event, ['editFieldTitle', 'editFieldDescription', 'editFieldCode', 'skip'])">
                        @active.Title
                    </a>
                </td>
                <td>
                    @active.Description
                </td>
                <td>
                    @active.CodeFXI
                </td>
            </tr>
        }
    </table>
</div>